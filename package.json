{
  "name": "hdl_editor",
  "version": "0.1.0",
  "description": "graphical editor for Verilog-based hardware design",
  "main": "app/main.js",
  "scripts": {
    "test": "mocha",
    "test:all": "electron-mocha \"./electron_unit_test/**/*test_*.js\"",
    "test:one": "electron-mocha electron_unit_test/test_launch.js",
    "start": "cd src/Main && dotnet fable webpack --port free -- -w --config ../../webpack.config.js",
    "build": "cd src/Main && dotnet fable webpack --port free -- -p --config ../../webpack.config.js",
    "build:webpack": "webpack",
    "launch": "electron .",
    "release": "electron-packager . --out release --overwrite"
  },
  "keywords": [
    "HDL"
  ],
  "author": "Lei Wang (Wilson)",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "css-loader": "^2.1.1",
    "electron": "^4.1.3",
    "electron-chromedriver": "^5.0.0-beta.1",
    "electron-packager": "^13.1.1",
    "fable-compiler": "^2.2.3",
    "fable-loader": "^2.1.5",
    "fable-utils": "^1.2.0",
    "file-loader": "^3.0.1",
    "mocha": "^6.0.2",
    "spectron": "^5.0.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "dependencies": {
    "@types/mocha": "^5.2.6",
    "babel-runtime": "^6.26.0",
    "electron-mocha": "^7.0.0"
  }
}
